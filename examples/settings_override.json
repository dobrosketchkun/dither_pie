{
    "_comment": "Settings-only config for use with input override",
    "_usage": "python ../dither_pie.py settings_override.json your_file.png",
    "input": "dummy",
    "output": "dummy",
    "pixelization": {
        "enabled": true,
        "method": "regular",
        "max_size": 64
    },
    "dithering": {
        "enabled": true,
        "mode": "bayer"
    },
    "palette": {
        "source": "median_cut",
        "num_colors": 16,
        "use_gamma": false
    },
    "final_resize": {
        "enabled": false,
        "multiplier": 2
    }
}

